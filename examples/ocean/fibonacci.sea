using std.annotation.memoize
# imports the trace annotation
using std.annotation.trace

@Trace
@Memoize
function fib (input: i64) -> (result: i64) {
  if input <= 1 {
    result = input;
  } else {
    result = fib(input - 1) + fib(input - 2);
  }
}

